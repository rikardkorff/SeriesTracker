<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seriestracker.controllers.editGenreController">
   <children>
      <TableView fx:id="genreTable" editable="true" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="genreColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" onEditCommit="#onEdited" prefWidth="100.0" text="Genre" />
        </columns>
      </TableView>
      <ButtonBar layoutX="237.0" layoutY="350.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#onSave" text="Save" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#onClose" text="Close" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
